<my-component-dialog
bind:determineevent="onMyEvent"
dialog-hidden="{{isHidden}}"
title-msg="{{titleMsg}}"
cancle-btn="{{cancleBtn}}"
author-title="{{authorTitle}}"
sure-msg="{{sureMsg}}"
bind:openevent="sureBtn"
/> 


